  s_TRS2_NOT_CMD_Rev : BOOL ;	//Not command reverse
  s_TRS2_OS_Dir_Chng : BOOL ;	//One shot direction change
  s_TRS2_HW_Req_Halt : BOOL ;	//HW request halt
  s_TRS2_Ext_Req_Halt : BOOL ;	//Internal resquest halt
  s_TRS2_Ready_To_Restart : BOOL ;	//Ready to restart to system control
  s_TRS2_Not_Automatic : BOOL ;	//Not automatic status report from hardware layer
