FUNCTION_BLOCK "FB_CIZ03_F"
TITLE =%version: 1.20 % CN: 55 
//Function:
//Control/fixed part of a Check-In Zone which consists of two 2-directional conveyors and one 
//single direction conveyor
//
//History:
//This version  %created_by: nlhdl %
//              %date_created: Monday, January 29, 2018 4:29:04 PM %
//              %release: VI_RAW/RA_Dev %
//
//Modification History:                                   By:    Date:     Rev:
//----------------------------------------------------  -------  --------  -----
//bhs_plf#5140: CIZ Unexpected item detection Restart   nlHdL    22-01-18   1.19
//ra#27330: CIZ03/Induct deadlock after stop command    nlHdL    12-01-18   1.18
//veg#1546: Desk on not work if TRS3 not started        nlHdL    12-10-17   1.17
//ra#24263: Signal light blink on error and desk off    nlHdL    09-10-17   1.16
//ina#6140: CIZ error suppression on ASI error HW       nlHdL    06-09-17   1.15
//ra#24634: Update DAT Layer                            nlHdL    01-09-17   1.14
//ina#6140: CIZ03 section status stopped                nlHdL    01-09-17   1.13
//ina#6140: CIZ02 section status stopped                nlHdL    11-07-17   1.12
//ra#21886: Added TRS3 succesive missings to TRS3 error nlSdW    07-06-17   1.11
//bhs_plf#1740: Add desk active informational           nlSdW    31-05-17   1.10
//ra#21569: Removed CIZ_Active condition for blink SL   nlpsal   06-04-17   1.09
//Die-forward when KS is not auto
//ra#21747: Update according to product book            nldy     03-04-17   1.08
//ra#20258: Wrong ASI module for Maintenance switch     nlHdL    30-01-17   1.07
//ra#47812: Flow control configuration                  nlJClo   24-01-17   1.06
//ra#18817: Clear PLT function Update                   inajos   07-12-16   1.05
//ra#18500: output ST Not Automatic rename too not auto nlHdL    01-12-16   1.04
//ra#17285:correct network labels                       nlJClo   19-09-16   1.03
//ra#17174:Removed not used inputs                      nlJClo   19-09-16   1.02
//ra#38710:Initial version (based on FB_CIZ03 v. 0.06)  nlJClo   01-06-16   1.01
//
//Description:
//TRS1 and TRS2 have two PECs each, one on each side of the conveyor. 
//TRS3 has one PEC, only on the EOS.
//
//View from above (conveyor):
//     |                             |                                     |
// +---+-----------------------------+---+ +-------------------------------+---+
// |   |   ---- Forward ---->        |   | |     ---- Forward ---->        |   |  
//           
// |   |  TRS01/TRS02                |   | |      TRS03                    |   | 
// |   |   <--- Reverse -----        |   | |                               |   | 
// +---+-----------------------------+---+ +-------------------------------+---+
//     |                             |                                     |      
//                   
//     ^                             ^                                     ^
//PEC upstream               PEC downstream                       PEC downstream
//
FAMILY : RAW
VERSION : 0.0


VAR_INPUT
  i_Request_Induct : BOOL ;	//Request to induct bag in window
  i_ID : DWORD ;	//ID Record HEX AAAA - ZZSS/OO.
  i_CMD_Enable : BOOL ;	//CMD: Enable command
  i_CMD_Start : BOOL ;	//CMD: Start command
  i_CMD_Reset : BOOL ;	//CMD: Reset command
  i_CMD_Send_Stat : BOOL ;	//CMD: Send Statistical
  i_Safety_OK : BOOL  := TRUE;	//Safety is OK signal
  i_Available_IO : BOOL  := TRUE;	//Signal for indication IO is OK (no Fieldbus or IO error)
  i_PBB_ASI_NotPrsnt : BOOL ;	//ASI is Not Present
  i_PBB_ASI_Error : BOOL ;	//ASI has an Error
  i_TRS1_ASI_NotPrsnt : BOOL ;	//ASI is Not Present
  i_TRS1_ASI_Error : BOOL ;	//ASI has an Error
  i_TRS2_ASI_NotPrsnt : BOOL ;	//ASI is Not Present
  i_TRS2_ASI_Error : BOOL ;	//ASI has an Error
  i_LCC_ASI_NotPrsnt : BOOL ;	//ASI is Not Present
  i_LCC_ASI_Error : BOOL ;	//ASI has an Error
  i_TRS3_ASI_NotPrsnt : BOOL ;	//ASI is Not Present
  i_TRS3_ASI_Error : BOOL ;	//ASI has an Error
  i_CFG_Double_Dispatch : BOOL ;	//CFG: Double dispatch enabled
  i_KS_IO_Desk_On : BOOL ;	//HW: Key-Switch check-in desk switched on
  i_PB_IO_Label : BOOL ;	//HW: Push Button Label
  i_PB_IO_Dispatch : BOOL ;	//HW: Push Button Dispatch
  i_PB_IO_Reverse : BOOL ;	//HW: Push Button Reverse
  i_KS_IO_Automatic_Mode : BOOL ;	//HW: Cabinet key switch automatic mode
  i_Ind_DB_PLT : BLOCK_DB ;	//Data Structure: Induct Position DataBlock (Dummy for induct)
  i_Ind_DB_PLT_Merge : BLOCK_DB ;	//Data Structure: Induct Position DataBlock (Dummy for induct)
  i_TRS1_Setting : "UDT_TRS_Bas2dir_Sett";	//Data Structure: Setting Record TRS1
  i_TRS1_ID : DWORD ;	//ID Record HEX AAAA - ZZSS/OO.
  i_TRS1_DB_PLT : BLOCK_DB ;	//Data Structure: Position DataBlock TRS1
  i_TRS1_CFG_Conv_Speed : INT ;	//CFG: [mm/sec] Conveyor Speed
  i_TRS1_CFG_Run_Out_Dist : INT ;	//CFG: [cm] Run out distance for the section
  i_TRS1_IO_NOT_PEC_1 : BOOL ;	//HW: Fail safe signal of Photo Electric Cell (TRUE=No product detected)
  i_TRS1_IO_NOT_PEC_2 : BOOL ;	//HW: Fail safe signal of Photo Electric Cell (TRUE=No product detected)
  i_TRS1_IO_CON_OK : BOOL ;	//HW: Fail safe signal Maintenance Switch on (TRUE=Motor switched on)
  i_TRS1_IO_NOT_THO : BOOL ;	//HW: Thermal overload signal (No Thermal overload=TRUE)  
  i_TRS2_Setting : "UDT_TRS_Bas2dir_Sett";	//Data Structure: Setting Record TRS2
  i_TRS2_ID : DWORD ;	//ID Record HEX AAAA - ZZSS/OO.
  i_TRS2_DB_PLT : BLOCK_DB ;	//Data Structure: Position DataBlock TRS2
  i_TRS2_CFG_Conv_Speed : INT ;	//CFG: [mm/sec] Conveyor Speed
  i_TRS2_CFG_Run_Out_Dist : INT ;	//CFG: [cm] Run out distance for the section
  i_TRS2_IO_NOT_PEC_1 : BOOL ;	//HW: Fail safe signal of Photo Electric Cell (TRUE=No product detected)
  i_TRS2_IO_NOT_PEC_2 : BOOL ;	//HW: Fail safe signal of Photo Electric Cell (TRUE=No product detected)
  i_TRS2_IO_CON_OK : BOOL ;	//HW: Fail safe signal Maintenance Switch on (TRUE=Motor swiched on)
  i_TRS2_IO_NOT_THO : BOOL ;	//HW: Thermal overload signal (No Thermal overload=TRUE) 
  i_TRS3_DB_PLT : BLOCK_DB ;	//Data Structure: Position DataBlock TRS3
  i_TRS3_IO_NOT_PEC : BOOL ;	//HW: Fail safe signal of Photo Electric Cell (TRUE=No product detected)
  i_TRS3_IO_CON_OK : BOOL ;	//HW: Fail safe signal Maintenance Switch on (TRUE=Motor swiched on)
  i_TRS3_EOS_PEC_Blockage : BOOL ;	//ST: PEC Blockage
  i_TRS3_EOS_PEC_Miss : BOOL ;	//ST: PEC successive missings
  i_TRS3_ST_THO : BOOL ;	//ST: Thermal overload 
  i_TRS3_ST_Not_Auto : BOOL ;	//ST  The motor is reporting not automatic
  i_TRS3_ST_Started : BOOL ;	//ST: Started
  i_TRS3_MTR_Run : BOOL ;	// Motor TRS is running
END_VAR
