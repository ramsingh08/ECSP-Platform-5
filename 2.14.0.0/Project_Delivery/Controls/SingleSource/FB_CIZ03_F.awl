BEGIN
NETWORK
TITLE =AA: Determine PLT DB's
//Determine when merge section is operational
      OPN   #i_Ind_DB_PLT_Merge; 

      A     DBX   10.5; // Retrieve section operational signal
      =     #s_Merge_Operational; 

      OPN   #i_TRS1_DB_PLT; 
      AN    DBX   10.7; 
      =     #s_TRS1_PEC_Latch_PEC_1; // Retrieve PEC latch signal

      OPN   #i_TRS2_DB_PLT; 
      AN    DBX   10.7; 
      =     #s_TRS2_PEC_Latch_PEC_1; // Retrieve PEC latch signal

      OPN   #i_TRS3_DB_PLT; 
      AN    DBX   10.7; 
      =     #s_TRS3_PEC_Latch_PEC; // Retrieve PEC latch signal

      A     #i_Available_IO; 
      A     #i_TRS1_IO_CON_OK; 
      A     #i_TRS2_IO_CON_OK; 
      A     #i_TRS3_IO_CON_OK; 
      AN    #i_PBB_ASI_NotPrsnt; 
      AN    #i_PBB_ASI_Error; 
      AN    #i_TRS1_ASI_NotPrsnt; 
      AN    #i_TRS1_ASI_Error; 
      AN    #i_TRS2_ASI_NotPrsnt; 
      AN    #i_TRS2_ASI_Error; 
      AN    #i_LCC_ASI_NotPrsnt; 
      AN    #i_LCC_ASI_Error; 
      AN    #i_TRS3_ASI_NotPrsnt; 
      AN    #i_TRS3_ASI_Error; 
      =     #t_IO_Available; 
NETWORK
TITLE =BA: Handle electrical input statuses
//Some errors and statuses are coming directly from electrical inputs.
//If the IO becomes not available, those inputs must be ignored, to avoid 
//(as consequence of) all kind of meaningless status changes, which are indicated 
//to HMI.
      O     #i_CMD_Reset; // Reset ASI errors
      ON    #i_Available_IO; 
      R     #o_ST_PBB_ASI_NotPrsnt; 
      R     #o_ST_PBB_ASI_Error; 
      R     #o_ST_TRS1_ASI_NotPrsnt; 
      R     #o_ST_TRS1_ASI_Error; 
      R     #o_ST_TRS2_ASI_NotPrsnt; 
      R     #o_ST_TRS2_ASI_Error; 
      R     #o_ST_LCC_ASI_NotPrsnt; 
      R     #o_ST_LCC_ASI_Error; 
      R     #o_ST_TRS3_ASI_NotPrsnt; 
      R     #o_ST_TRS3_ASI_Error; 


      A     #i_PBB_ASI_NotPrsnt; // Set ASI PBB errors
      A     #i_Available_IO; 
      S     #o_ST_PBB_ASI_NotPrsnt; 

      A     #i_PBB_ASI_Error; 
      A     #i_Available_IO; 
      S     #o_ST_PBB_ASI_Error; 

      A     #i_TRS1_ASI_NotPrsnt; 
      A     #i_Available_IO; 
      S     #o_ST_TRS1_ASI_NotPrsnt; 

      A     #i_TRS1_ASI_Error; 
      A     #i_Available_IO; 
      S     #o_ST_TRS1_ASI_Error; 

      A     #i_TRS2_ASI_NotPrsnt; 
      A     #i_Available_IO; 
      S     #o_ST_TRS2_ASI_NotPrsnt; 

      A     #i_TRS2_ASI_Error; 
      A     #i_Available_IO; 
      S     #o_ST_TRS2_ASI_Error; 

      A     #i_LCC_ASI_NotPrsnt; 
      A     #i_Available_IO; 
      S     #o_ST_LCC_ASI_NotPrsnt; 

      A     #i_LCC_ASI_Error; 
      A     #i_Available_IO; 
      S     #o_ST_LCC_ASI_Error; 

      A     #i_TRS3_ASI_NotPrsnt; 
      A     #i_Available_IO; 
      S     #o_ST_TRS3_ASI_NotPrsnt; 

      A     #i_TRS3_ASI_Error; 
      A     #i_Available_IO; 
      S     #o_ST_TRS3_ASI_Error; 

NETWORK
TITLE =CA: KS automatic mode
//LAD!
//Determine when the key switch of the cabinet is in automatic mode
      AN    #i_KS_IO_Automatic_Mode; // If KS in 'Automatic' mode
      A     #t_IO_Available; // AND IO available
      AN    #o_ST_LCC_ASI_NotPrsnt; // AND ASI LCC present
      AN    #o_ST_LCC_ASI_Error; // AND ASI LCC not in error
      =     #o_ST_Desk_Disabled; // THEN KS in 'Automatic' position
NETWORK
TITLE =DA: CIZ active
//Determine when the CIZ is activated
      A     #i_KS_IO_Desk_On; 
      AN    #o_ST_Unexp_Detection; 
      FP    #s_FP_KS_IO_Desk_On; 
      =     #t_OS_KS_IO_Desk_On; 

      A(    ; 
      A     #i_CMD_Start; 
      A     #i_KS_IO_Desk_On; //Desk on & Start command
      O     #t_OS_KS_IO_Desk_On; //Or Positive flank of keyswitch
      )     ; 
      AN    #o_ST_Desk_Disabled; 
      AN    #s_CIZ_Error; 
      AN    #s_TRS1_Error; 
      AN    #s_TRS2_Error; 
      AN    #s_TRS3_Error; 
      A     #t_IO_Available; 
      A     #i_Safety_OK; 
      A     #i_CMD_Enable; 
      S     #s_CIZ_Active; 
      R     #s_CIZ_Inactive; 

      A     #s_CIZ_Active; 
      =     #o_ST_Desk_Active; 
NETWORK
TITLE =EA: CIZ inactive
//LAD!
//Determine when the CIZ is deactivated
      AN    #i_KS_IO_Desk_On; 
      AN    #s_TRS3_Dispatch_Busy; 
      O     #o_ST_Desk_Disabled; 
      ON    #t_IO_Available; 
      ON    #i_Safety_OK; 
      ON    #i_CMD_Enable; 
      S     #s_CIZ_Inactive; 
      R     #s_CIZ_Active; 
NETWORK
TITLE =FA: Reset signal for PEC blockage errors

      O     #i_KS_IO_Desk_On; 
      FN    #s_FN_KS_IO_Desk_On; 
      O     #i_CMD_Reset; 
      =     #t_Reset_PEC_Blockage; 

NETWORK
TITLE =HA: Equipment calls for TRS1 belt

      NOP   0; 

      CALL #s_TRS1_FB_EQP_Switch_Dir (
           i_DB_PLT                 := #i_TRS1_DB_PLT,
           i_DB_PLT_DwnStr          := #i_TRS2_DB_PLT,
           i_CMD_Reverse            := #s_TRS1_Req_MTR_Rev,
           o_OS_Direction_Change    := #s_TRS1_OS_Dir_Chng,
           o_CMD_MTR_Reverse        := #s_TRS1_CMD_Rev);

// Equipment layer PEC EOS
      CALL #s_TRS1_FB_PEC_2Dir_EOS (
           i_Setting                := #i_TRS1_Setting.PEC_1_2D,
           i_DB_PLT                 := #i_TRS1_DB_PLT,
           i_DB_Event               := "DB_Event_Section",
           i_Dwnstr_PLT_Pos         := 0,
           i_CFG_Blockage_Length    := 200,
           i_CFG_Delay_On_Filter    := 0,
           i_CFG_Delay_Off_Filter   := 0,
           i_CFG_Min_Product_Length := 0,
           i_CFG_Track_Present      := FALSE,
           i_CFG_Reset_Block_PEC_Oc := FALSE,
           i_IO_NOT_PEC             := #i_TRS1_IO_NOT_PEC_1,
           i_CFG_Preset_PEC_Latch   := TRUE,
           i_Eqp_Available_IO       := #t_IO_Available,
           i_CMD_Reset              := #t_Reset_PEC_Blockage,
           i_Disable_Update_Funct   := #s_TRS1_CMD_Rev,
           o_PEC_Position           := #s_Tl_Offset_01_FW,
           o_ST_PEC_Blk             := #o_TRS1_ST_PEC1_Blk);

      AN    #s_TRS1_CMD_Rev; 
      =     #s_TRS1_NOT_CMD_Rev; 

// Equipment layer PEC SOS
      CALL #s_TRS1_FB_PEC_2Dir_SOS (
           i_Setting                := #i_TRS1_Setting.PEC_2_2D,
           i_DB_PLT                 := #i_TRS1_DB_PLT,
           i_DB_Event               := "DB_Event_Section",
           i_Dwnstr_PLT_Pos         := 0,
           i_CFG_Blockage_Length    := 300,
           i_CFG_Delay_On_Filter    := 0,
           i_CFG_Delay_Off_Filter   := 0,
           i_CFG_Min_Product_Length := 0,
           i_CFG_Track_Present      := FALSE,
           i_CFG_Reset_Block_PEC_Oc := FALSE,
           i_IO_NOT_PEC             := #i_TRS1_IO_NOT_PEC_2,
           i_CFG_Preset_PEC_Latch   := #s_TRS1_Not_Automatic,
           i_Eqp_Available_IO       := #t_IO_Available,
           i_CMD_Reset              := #t_Reset_PEC_Blockage,
           i_Disable_Update_Funct   := #s_TRS1_NOT_CMD_Rev,
           o_PEC_Position           := #s_Tl_Offset_01_RV,
           o_ST_PEC_Blk             := #o_TRS1_ST_PEC2_Blk);

//Calculated standard start-up for section
      L     #i_TRS1_CFG_Run_Out_Dist; 
      L     2; 
      /I    ; 
      T     #t_TRS_Start_Up_Dist; 

      A     #s_CIZ_Error; 
      O     #s_TRS1_Error; 
      O(    ; 
      A     #s_TRS2_Error; 
      A     #s_TRS1_HandOver_Busy; 
      )     ; 
      O     #s_TRS1_OS_Dir_Chng; 
      O     #s_TRS1_HW_Req_Halt; 
      =     #s_TRS1_Ext_Req_Halt; 

      AN    #s_TRS1_CMD_Rev; 
      JCN   HA01; 

      L     #s_Tl_Offset_01_FW; 
      T     #s_Tl_Offset_01; 

//Load offset forward
      L     #i_TRS1_Setting.Basic_FWD.Offset; 
      T     #t_TRS1_Basic_Offset; 

HA01: A     #s_TRS1_CMD_Rev; 
      JCN   HA02; 

      L     #s_Tl_Offset_01_RV; 
      T     #s_Tl_Offset_01; 

//Load offset reverse
      L     #i_TRS1_Setting.Basic_REV.Offset; 
      T     #t_TRS1_Basic_Offset; 

HA02: CALL #s_TRS1_FB_EQP_Lean (
           i_ID_Section             := #i_TRS1_ID,
           i_Basic_Offset           := #t_TRS1_Basic_Offset,
           i_DB_PLT                 := #i_TRS1_DB_PLT,
           i_DB_PLT_DwnStr          := #i_TRS2_DB_PLT,
           i_DB_Event               := "DB_Event_Section",
           i_CFG_Conveyor_Speed     := #i_TRS1_CFG_Conv_Speed,
           i_CFG_Run_Out_Distance   := #i_TRS1_CFG_Run_Out_Dist,
           i_CFG_Start_Up_Distance  := #t_TRS_Start_Up_Dist,
           i_PPI_Displacement       := 0,
           i_Dwnstr_PLT_Pos         := #s_Tl_Offset_01,
           i_CFG_Del_Window_Codes   := FALSE,
           i_Available_IO           := #t_IO_Available,
           i_Safety_OK              := #i_Safety_OK,
           i_CMD_Enable             := #i_CMD_Enable,
           i_CMD_Start              := #i_CMD_Start,
           i_Ext_Req_Run            := #s_TRS1_Req_MTR_Run,
           i_Ext_Req_Halt           := #s_TRS1_Ext_Req_Halt,
           i_OS_Direction_Change    := #s_TRS1_OS_Dir_Chng,
           i_Clr_PLT_Data           := #s_TRS1_Not_Automatic,
           o_External               := #o_TRS1_EXT_Outputs,
           o_Motor_Run              := #s_TRS1_MTR_Run,
           o_ST_Items               := #o_TRS1_ST_Items,
           o_ST_Running             := #o_TRS1_ST_Running,
           o_ST_Started             := #o_TRS1_ST_Started,
           o_ST_Request_Halt        := #o_TRS1_ST_Req_Halt,
           o_ST_Request_Stop        := #o_TRS1_ST_Req_Stop,
           o_Ready_To_Restart       := #s_TRS1_Ready_To_Restart);

      AN    #s_CIZ_Active; 
      R     #o_TRS1_ST_Started; 

      A     #i_Available_IO; 
      AN    #o_ST_TRS1_ASI_NotPrsnt; // AND ASI LMS present
      AN    #o_ST_TRS1_ASI_Error; // AND ASI LMS not in error
      =     #t_Available_HW; 

      A     #i_KS_IO_Automatic_Mode; 
      A     #i_TRS1_IO_CON_OK; 
      =     #t_TRS1_Automatic; 

      CALL #s_TRS1_HW_MTR (
           i_CMD_Send_Stat          := #i_CMD_Send_Stat,
           i_Event_DB               := "DB_Event_Section",
           i_CFG_Motor_Switch_Time  := 300,
           i_Available_IO           := #t_Available_HW,
           i_IO_MS_Automatic_Mode   := #t_TRS1_Automatic,
           i_IO_NOT_THO             := #i_TRS1_IO_NOT_THO,
           i_CMD_Reset              := #i_CMD_Reset,
           i_CMD_Req_Motor_Run      := #s_TRS1_MTR_Run,
           i_CMD_Req_Motor_Reverse  := #s_TRS1_CMD_Rev,
           o_IO_MTR_Fwd             := #o_TRS1_IO_MTR_FWD,
           o_IO_MTR_Rev             := #o_TRS1_IO_MTR_REV,
           o_ST_MS_Not_Automatic    := #s_TRS1_Not_Automatic,
           o_ST_THO                 := #o_TRS1_ST_THO,
           o_Req_Halt               := #s_TRS1_HW_Req_Halt);

      A     #s_TRS1_Not_Automatic; 
      =     #o_TRS1_ST_Not_Auto; 

      CALL #s_TRS1_FB_DAT (
           i_ID_Section             := #i_TRS1_ID,
           i_DB_Event               := "DB_Event_Section",
           i_PLC_Log_FIFO_DB        := "DB_LOG_Event_FIFO",
           i_CFG_BPI_Log_Level      := "DB_Setting".common.External_Log_Level,
           i_CFG_PLC_Log_Level      := "DB_Setting".common.Internal_Log_Level,
           i_CFG_PLC_Log_Route_Msgs := FALSE,
           i_CFG_PLC_LOG_BPI_Stat   := FALSE,
           i_CFG_PLC_No_Msg_To_Rout := FALSE,
           i_CFG_PLC_No_Msg_To_BPI  := FALSE);


NETWORK
TITLE =IA: Equipment calls for TRS2 belt

      NOP   0; 

      CALL #s_TRS2_FB_EQP_Switch_Dir (
           i_DB_PLT                 := #i_TRS2_DB_PLT,
           i_DB_PLT_UpStr           := #i_TRS1_DB_PLT,
           i_DB_PLT_DwnStr          := #i_TRS3_DB_PLT,
           i_CMD_Reverse            := #s_TRS2_Req_MTR_Rev,
           o_OS_Direction_Change    := #s_TRS2_OS_Dir_Chng,
           o_CMD_MTR_Reverse        := #s_TRS2_CMD_Rev);

// Equipment layer PEC EOS
      CALL #s_TRS2_FB_PEC_2Dir_EOS (
           i_Setting                := #i_TRS2_Setting.PEC_1_2D,
           i_DB_PLT                 := #i_TRS2_DB_PLT,
           i_DB_Event               := "DB_Event_Section",
           i_Dwnstr_PLT_Pos         := 0,
           i_CFG_Blockage_Length    := 150,
           i_CFG_Delay_On_Filter    := 0,
           i_CFG_Delay_Off_Filter   := 0,
           i_CFG_Min_Product_Length := 0,
           i_CFG_Track_Present      := FALSE,
           i_CFG_Reset_Block_PEC_Oc := FALSE,
           i_IO_NOT_PEC             := #i_TRS2_IO_NOT_PEC_1,
           i_CFG_Preset_PEC_Latch   := #s_TRS2_Not_Automatic,
           i_Eqp_Available_IO       := #t_IO_Available,
           i_CMD_Reset              := #t_Reset_PEC_Blockage,
           i_Disable_Update_Funct   := #s_TRS2_CMD_Rev,
           o_PEC_Position           := #s_Tl_Offset_02_FW,
           o_ST_PEC_Blk             := #o_TRS2_ST_PEC1_Blk);

      AN    #s_TRS2_CMD_Rev; 
      =     #s_TRS2_NOT_CMD_Rev; 

// Equipment layer PEC SOS
      CALL #s_TRS2_FB_PEC_2Dir_SOS (
           i_Setting                := #i_TRS2_Setting.PEC_2_2D,
           i_DB_PLT                 := #i_TRS2_DB_PLT,
           i_DB_Event               := "DB_Event_Section",
           i_Dwnstr_PLT_Pos         := 0,
           i_CFG_Blockage_Length    := 300,
           i_CFG_Delay_On_Filter    := 0,
           i_CFG_Delay_Off_Filter   := 0,
           i_CFG_Min_Product_Length := 0,
           i_CFG_Track_Present      := FALSE,
           i_CFG_Reset_Block_PEC_Oc := FALSE,
           i_IO_NOT_PEC             := #i_TRS2_IO_NOT_PEC_2,
           i_CFG_Preset_PEC_Latch   := #s_TRS2_Not_Automatic,
           i_Eqp_Available_IO       := #t_IO_Available,
           i_CMD_Reset              := #t_Reset_PEC_Blockage,
           i_Disable_Update_Funct   := #s_TRS2_NOT_CMD_Rev,
           o_PEC_Position           := #s_Tl_Offset_02_RV,
           o_ST_PEC_Blk             := #o_TRS2_ST_PEC2_Blk);

//Calculated standard start-up for section
      L     #i_TRS2_CFG_Run_Out_Dist; 
      L     2; 
      /I    ; 
      T     #t_TRS_Start_Up_Dist; 

      A     #s_CIZ_Error; 
      O     #s_TRS2_Error; 
      O(    ; 
      A     #s_TRS1_Error; 
      A     #s_TRS1_HandOver_Busy; 
      )     ; 
      O     #s_TRS2_OS_Dir_Chng; 
      O     #s_TRS2_HW_Req_Halt; 
      =     #s_TRS2_Ext_Req_Halt; 

      AN    #s_TRS2_CMD_Rev; 
      JCN   IA01; 

      L     #s_Tl_Offset_02_FW; 
      T     #s_Tl_Offset_02; 

//Load offset forward
      L     #i_TRS2_Setting.Basic_FWD.Offset; 
      T     #t_TRS2_Basic_Offset; 

IA01: A     #s_TRS2_CMD_Rev; 
      JCN   IA02; 

      L     #s_Tl_Offset_02_RV; 
      T     #s_Tl_Offset_02; 

//Load offset reverse
      L     #i_TRS2_Setting.Basic_REV.Offset; 
      T     #t_TRS2_Basic_Offset; 

IA02: CALL #s_TRS2_FB_EQP_Lean (
           i_ID_Section             := #i_TRS2_ID,
           i_Basic_Offset           := #t_TRS2_Basic_Offset,
           i_DB_PLT                 := #i_TRS2_DB_PLT,
           i_DB_PLT_DwnStr          := #i_TRS3_DB_PLT,
           i_DB_Event               := "DB_Event_Section",
           i_CFG_Conveyor_Speed     := #i_TRS2_CFG_Conv_Speed,
           i_CFG_Run_Out_Distance   := #i_TRS2_CFG_Run_Out_Dist,
           i_CFG_Start_Up_Distance  := #t_TRS_Start_Up_Dist,
           i_PPI_Displacement       := 0,
           i_Dwnstr_PLT_Pos         := #s_Tl_Offset_02,
           i_CFG_Del_Window_Codes   := FALSE,
           i_Available_IO           := #t_IO_Available,
           i_Safety_OK              := #i_Safety_OK,
           i_CMD_Enable             := #i_CMD_Enable,
           i_CMD_Start              := #i_CMD_Start,
           i_Ext_Req_Run            := #s_TRS2_Req_MTR_Run,
           i_Ext_Req_Halt           := #s_TRS2_Ext_Req_Halt,
           i_OS_Direction_Change    := #s_TRS2_OS_Dir_Chng,
           i_Clr_PLT_Data           := #s_TRS2_Not_Automatic,
           o_External               := #o_TRS2_EXT_Outputs,
           o_Motor_Run              := #s_TRS2_MTR_Run,
           o_ST_Items               := #o_TRS2_ST_Items,
           o_ST_Running             := #o_TRS2_ST_Running,
           o_ST_Started             := #o_TRS2_ST_Started,
           o_ST_Request_Halt        := #o_TRS2_ST_Req_Halt,
           o_ST_Request_Stop        := #o_TRS2_ST_Req_Stop,
           o_Ready_To_Restart       := #s_TRS2_Ready_To_Restart);

      AN    #s_CIZ_Active; 
      R     #o_TRS2_ST_Started; 

      A     #i_Available_IO; 
      AN    #o_ST_TRS2_ASI_NotPrsnt; // AND ASI LMS present
      AN    #o_ST_TRS2_ASI_Error; // AND ASI LMS not in error
      =     #t_Available_HW; 

      A     #i_KS_IO_Automatic_Mode; 
      A     #i_TRS2_IO_CON_OK; 
      =     #t_TRS2_Automatic; 

      CALL #s_TRS2_HW_MTR (
           i_CMD_Send_Stat          := #i_CMD_Send_Stat,
           i_Event_DB               := "DB_Event_Section",
           i_CFG_Motor_Switch_Time  := 300,
           i_Available_IO           := #t_Available_HW,
           i_IO_MS_Automatic_Mode   := #t_TRS2_Automatic,
           i_IO_NOT_THO             := #i_TRS2_IO_NOT_THO,
           i_CMD_Reset              := #i_CMD_Reset,
           i_CMD_Req_Motor_Run      := #s_TRS2_MTR_Run,
           i_CMD_Req_Motor_Reverse  := #s_TRS2_CMD_Rev,
           o_IO_MTR_Fwd             := #o_TRS2_IO_MTR_FWD,
           o_IO_MTR_Rev             := #o_TRS2_IO_MTR_REV,
           o_ST_MS_Not_Automatic    := #s_TRS2_Not_Automatic,
           o_ST_THO                 := #o_TRS2_ST_THO,
           o_Req_Halt               := #s_TRS2_HW_Req_Halt);

      A     #s_TRS2_Not_Automatic; 
      =     #o_TRS2_ST_Not_Auto; 

      CALL #s_TRS2_FB_DAT (
           i_ID_Section             := #i_TRS2_ID,
           i_DB_Event               := "DB_Event_Section",
           i_PLC_Log_FIFO_DB        := "DB_LOG_Event_FIFO",
           i_CFG_BPI_Log_Level      := "DB_Setting".common.External_Log_Level,
           i_CFG_PLC_Log_Level      := "DB_Setting".common.Internal_Log_Level,
           i_CFG_PLC_Log_Route_Msgs := FALSE,
           i_CFG_PLC_LOG_BPI_Stat   := FALSE,
           i_CFG_PLC_No_Msg_To_Rout := FALSE,
           i_CFG_PLC_No_Msg_To_BPI  := FALSE);

NETWORK
TITLE =JA: control TRS3 belt

      NOP   0; 

//PEC will be discard in case of child detection (happend when CIZ is inactive)
      A     #s_CIZ_Inactive; 
      O     #i_TRS3_IO_NOT_PEC; 
      =     #o_TRS3_IO_NOT_PEC; 

      AN    #i_TRS3_MTR_Run; //Input TRS03 Run
      =     #s_TRS3_MTR_Not_Run; 

      CALL "FC_On_Dly_T_0.1s" (
           i_Start                  := #s_TRS3_MTR_Not_Run,
           i_Enable_Dly             := TRUE,
           i_Time_Value             := 10,
           i_Current_Value          := #s_MTR_Counter_value,
           o_Status                 := #s_MTR_TRS3_Not_Run_1sec,
           o_Current_Value          := #s_MTR_Counter_value);

      A     #s_MTR_TRS3_Not_Run_1sec; // If the motor of TRS3 is not running for more than 1 second
      AN    #s_CIZ_Inactive; // Only preset is zone is not inactive
      =     #o_TRS3_Preset_PEC_Latch; // Preset PEC latch signal 

      A     #s_CIZ_Error; 
      O     #s_TRS3_Error; 
      O(    ; 
      A     #s_TRS2_Error; 
      A     #s_TRS2_HandOver_Busy; 
      )     ; 
      O     #s_TRS3_HW_Req_Halt; 
      =     #o_TRS3_Ext_Req_Halt; 
NETWORK
TITLE =KA: CIZ control block call
//This block contains the manual control of the TRS1 and the dispatch sequence 
//for inducting on the CLZ.
      NOP   0; 

      A     #s_CIZ_Error; // If the CIZ has an error
      O     #s_TRS1_Error; // OR TRS1 has an error
      O     #s_TRS2_Error; // OR TRS2 has an error
      O     #s_TRS3_Error; // OR TRS3 has an error
      =     #s_Init_Sequencer; // Initialize the sequencer and set idle state

      CALL #s_FB_CIZ_Control (
           i_TRS1_DB_PLT            := #i_TRS1_DB_PLT,
           i_TRS2_DB_PLT            := #i_TRS2_DB_PLT,
           i_TRS3_DB_PLT            := #i_TRS3_DB_PLT,
           i_Event_DB               := "DB_Event_Zone",
           i_CIZ_Active             := #s_CIZ_Active,
           i_Request_Induct         := #i_Request_Induct,
           i_CFG_Double_Dispatch    := #i_CFG_Double_Dispatch,
           i_KS_IO_Desk_On          := #i_KS_IO_Desk_On,
           i_PB_IO_Label_Weight     := #i_PB_IO_Label,
           i_PB_IO_Dispatch         := #i_PB_IO_Dispatch,
           i_PB_IO_Reverse          := #i_PB_IO_Reverse,
           i_TRS1_IO_NOT_PEC_Upstrm := #i_TRS1_IO_NOT_PEC_2,
           i_TRS1_NOT_PEC_Downstr   := #s_TRS1_PEC_Latch_PEC_1,
           i_TRS2_IO_NOT_PEC_Upstrm := #i_TRS2_IO_NOT_PEC_2,
           i_TRS2_NOT_PEC_Downstr   := #s_TRS2_PEC_Latch_PEC_1,
           i_TRS3_NOT_PEC_Downstr   := #s_TRS3_PEC_Latch_PEC,
           i_Init_Sequencer         := #s_Init_Sequencer,
           i_Dispatch_Timeout       := 9000,
           o_TRS1_Req_MTR_Run       := #s_TRS1_Req_MTR_Run,
           o_TRS1_Req_MTR_Rev       := #s_TRS1_Req_MTR_Rev,
           o_TRS2_Req_MTR_Run       := #s_TRS2_Req_MTR_Run,
           o_TRS2_Req_MTR_Rev       := #s_TRS2_Req_MTR_Rev,
           o_TRS3_Req_MTR_Run       := #o_TRS3_Req_MTR_Run,
           o_TRS1_HandOver_Busy     := #s_TRS1_HandOver_Busy,
           o_TRS2_HandOver_Busy     := #s_TRS2_HandOver_Busy,
           o_TRS3_Dispatch_Busy     := #s_TRS3_Dispatch_Busy,
           o_TRS3_Ready_To_Induct   := #s_TRS3_Ready_To_Induct,
           o_ST_Length_Check        := #o_ST_Length_Check,
           o_IO_SL_Dispatch_Button  := #o_IO_SL_Dispatch_Button);

      CALL #s_CIZ_FB_DAT (
           i_ID_Section             := #i_ID,
           i_DB_Event               := "DB_Event_Zone",
           i_PLC_Log_FIFO_DB        := "DB_LOG_Event_FIFO",
           i_CFG_BPI_Log_Level      := "DB_Setting".common.External_Log_Level,
           i_CFG_PLC_Log_Level      := "DB_Setting".common.Internal_Log_Level,
           i_CFG_PLC_Log_Route_Msgs := FALSE,
           i_CFG_PLC_LOG_BPI_Stat   := FALSE,
           i_CFG_PLC_No_Msg_To_Rout := FALSE,
           i_CFG_PLC_No_Msg_To_BPI  := FALSE);

NETWORK
TITLE =LA: Child detection error
//When the section is inactive, started and the downstream PEC of the dispatch 
//belt is triggered the child detection is set. The child detection will be 
//resetted with use of the key switch.
      A     #s_CIZ_Inactive; 
      A     #s_Merge_Operational; 
      AN    #i_TRS3_IO_NOT_PEC; 
      AN    #o_TRS3_ST_PEC_Blk; 
      A     #t_IO_Available; 
      A     #i_Safety_OK; 
      S     #o_ST_Unexp_Detection; 

      A     #i_KS_IO_Desk_On; 
      ON    #t_IO_Available; 
      R     #o_ST_Unexp_Detection; 
NETWORK
TITLE =MA: PEC defect
//When a child detection is reset with use of the key switch but the PEC sensor 
//signal is still false, a PEC defect error will be generated. This prevents that 
//when the PEC is not working the collector belt stops every time because of a 
//child detection.
      A     #i_TRS3_IO_NOT_PEC; 
      R     #s_Unexp_Detection_Set; 

      A     #t_Reset_PEC_Blockage; 
      R     #s_PEC_Defect_PEC3; 

      A     #o_ST_Unexp_Detection; 
      S     #s_Unexp_Detection_Set; 

      A     #s_Unexp_Detection_Set; 
      AN    #o_ST_Unexp_Detection; 
      AN    #i_TRS3_IO_NOT_PEC; 
      A     #t_IO_Available; 
      S     #s_PEC_Defect_PEC3; 

      A     #i_TRS3_EOS_PEC_Blockage; 
      O     #s_PEC_Defect_PEC3; 
      =     #o_TRS3_ST_PEC_Blk; 


NETWORK
TITLE =NA: Desk on status indication light
//LAD!
//Indicates when the check-in desk is activated, deactivated or when the check-in 
//desk is having an error. When the check-in desk has an error the signal light 
//will indicate this by blinking.
      A     #t_IO_Available; 
      A(    ; 
      O     #i_KS_IO_Desk_On; 
      O     #s_CIZ_Active; 
      )     ; 
      A     #s_Init_Sequencer; //CIZ or TRS error
      O     #o_Die_Forward; // Needs to be shown when desk switched off
      A     "M_System_Clock_1.0_sec."; 
      O     ; 
      A     #s_CIZ_Active; 
      AN    #s_Init_Sequencer; //CIZ or TRS error
      =     #o_IO_SL_Desk_On; 
NETWORK
TITLE =OA: Bag length error indication light
//LAD!
//Writes the output for the length check signal light.
      A     #o_ST_Length_Check; 
      =     #o_IO_SL_Check_Error; 
NETWORK
TITLE =PA: Status error CIZ
//LAD!
//Collected errors for the CIZ error status
      AN    #t_IO_Available; 
      O     #o_ST_PBB_ASI_NotPrsnt; 
      O     #o_ST_PBB_ASI_Error; 
      O     #o_ST_TRS1_ASI_NotPrsnt; 
      O     #o_ST_TRS1_ASI_Error; 
      O     #o_ST_TRS2_ASI_NotPrsnt; 
      O     #o_ST_TRS2_ASI_Error; 
      O     #o_ST_LCC_ASI_NotPrsnt; 
      O     #o_ST_LCC_ASI_Error; 
      O     #o_ST_TRS3_ASI_NotPrsnt; 
      O     #o_ST_TRS3_ASI_Error; 
      O     #o_ST_Unexp_Detection; 
      =     #s_CIZ_Error; 
NETWORK
TITLE =QA: Status Error TRS1
//LAD!
      A     #o_TRS1_ST_PEC1_Blk; 
      O     #o_TRS1_ST_PEC2_Blk; 
      O     #o_TRS1_ST_THO; 
      O     #o_TRS1_ST_Not_Auto; 
      =     #s_TRS1_Error; 
NETWORK
TITLE =RA: Status Error TRS2
//LAD!
      A     #o_TRS2_ST_PEC1_Blk; 
      O     #o_TRS2_ST_PEC2_Blk; 
      O     #o_TRS2_ST_THO; 
      O     #o_TRS2_ST_Not_Auto; 
      =     #s_TRS2_Error; 
NETWORK
TITLE =SA: Status Error TRS3
//LAD!
      A     #o_TRS3_ST_PEC_Blk; 
      O     #i_TRS3_ST_THO; 
      O     #i_TRS3_ST_Not_Auto; 
      O     #i_TRS3_EOS_PEC_Miss; 
      =     #s_TRS3_Error; 
NETWORK
TITLE =TA: Die forward for collector zone
//LAD!
