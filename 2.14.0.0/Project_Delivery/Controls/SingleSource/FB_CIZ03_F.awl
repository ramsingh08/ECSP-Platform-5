  s_CIZ_FB_DAT : "FB_DAT";	//Block call static FB_DAT
  s_TRS1_FB_EQP_Switch_Dir : "FB_EQP_Switch_Dir";	//Block call static FB_EQP_Switch_Dir
  s_TRS1_FB_PEC_2Dir_EOS : "FB_EQP_PEC_2Dir";	//Block call statis FB_EQP_PEC_2Dir
  s_TRS1_FB_PEC_2Dir_SOS : "FB_EQP_PEC_2Dir";	//Block call static FB_EQP_PEC_2Dir
  s_TRS1_FB_EQP_Lean : "FB_EQP_Lean";	//Block call static FB_EQP_Lean
  s_TRS1_HW_MTR : "FB_HW_MTR_2DIR_MS";	//Block call static FB_HW_MTR
  s_TRS1_FB_DAT : "FB_DAT";	//Block call static FB_DAT
  s_TRS2_FB_EQP_Switch_Dir : "FB_EQP_Switch_Dir";	//Block call static FB_EQP_Switch_Dir
  s_TRS2_FB_PEC_2Dir_EOS : "FB_EQP_PEC_2Dir";	//Block call static FB_EQP_PEC
  s_TRS2_FB_PEC_2Dir_SOS : "FB_EQP_PEC_2Dir";	//Block call static FB_EQP_PEC
  s_TRS2_FB_EQP_Lean : "FB_EQP_Lean";	//Block call static FB_EQP_Lean
  s_TRS2_HW_MTR : "FB_HW_MTR_2DIR_MS";	//Block call static FB_HW_MTR
  s_TRS2_FB_DAT : "FB_DAT";	//Block call static FB_DAT
  s_Tl_Offset_01_FW : INT ;	//Position of the physical PEC in forward
  s_Tl_Offset_01_RV : INT ;	//Position of the physical PEC in reverse
  s_Tl_Offset_01 : INT ;	//Position of the physical PEC
